class Solution {
    void pushZerosToEnd(int[] arr) {
        // code here
        int index=0;
        int n=arr.length;
        for(int i=0;i<n;i++){
            if (arr[i]!=0){
                arr[index]=arr[i];
                index++;
            }
        }
        while(index<n){
            arr[index]=0;
            index++;
        }
    }
}
