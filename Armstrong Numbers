

// User function Template for Java
class Solution {
    static String armstrongNumber(int n) {
        // code here
        int temp=n;
        int r, sum=0;
        while(n>0){
            r=n%10;
            n=n/10 ;
            sum=sum+r*r*r;
        }
        if(temp==sum)
            return "true";
        else
         return "false";
        
    }
}
