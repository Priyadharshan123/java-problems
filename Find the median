

//User function Template for Java

class Solution
{
    public int find_median(int[] v)
    {
        // Code here
        int mid;
        Arrays.sort(v);
        int n=v.length;
        if(n%2 == 0){
            mid=n/2;
            return((v[mid]+ v[mid-1])/2);
        }
        else{
            mid = n/2;
            return v[mid];
        }
    }
}
